<div class="pin">
  <div class="well" id="pin-box" >
    <div class="well" id="equation-well">
      <p id="pinned-equation">$<%= feed_item.equation %>$</p>
    </div>
    <div class="description" id="pin-description"><%= feed_item.description %></div>
    <div class="timestamp" id="pin-timestamp">
      Posted: <%= feed_item.created_at %>.
    </div>
  </div>
</div>
